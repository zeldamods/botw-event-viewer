{"DemoMsg/Demo370_0:Demo370_0_Text000": {"contents": [{"control": {"kind": "sound2", "unknown": [7, 205]}}, {"control": {"kind": "sound", "unknown": [0, 1]}}, {"text": "*cough cough*"}, {"control": {"kind": "pause", "length": "short"}}, {"text": "\n*ahem*"}]}, "DemoMsg/Demo370_0:Demo370_0_Text001": {"contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Hudson..."}]}, "DemoMsg/Demo370_0:Demo370_0_Text002": {"contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Do you take Rhondson to be your wife,\nto have and to hold, in good times and\nin bad?"}]}, "DemoMsg/Demo370_0:Demo370_0_Text003": {"contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Do you take Hudson as your lawfully\nwedded husband, in sickness and\nin health?"}]}, "DemoMsg/Demo370_0:Demo370_0_Text004": {"contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "I do."}]}, "DemoMsg/Demo370_0:Demo370_0_Text005": {"contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Excellent."}]}, "DemoMsg/Demo370_0:Demo370_0_Text006": {"contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "And Rhondson..."}]}, "DemoMsg/Demo370_0:Demo370_0_Text007": {"contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "And do you also vow to..."}, {"control": {"kind": "pause", "length": "longer"}}, {"text": "name your\nchildren in accordance with the Bolson\nConstruction naming guidelines?"}]}, "DemoMsg/Demo370_0:Demo370_0_Text008": {"contents": [{"control": {"kind": "sound", "unknown": [0, 6]}}, {"text": "Wait a moment..."}, {"control": {"kind": "pause", "length": "longer"}}, {"text": " Those vows are not\ntraditional by anyone's standards!"}]}, "DemoMsg/Demo370_0:Demo370_0_Text009": {"contents": [{"control": {"kind": "sound", "unknown": [0, 6]}}, {"text": "What makes you say that?"}]}, "DemoMsg/Demo370_0:Demo370_0_Text010": {"contents": [{"control": {"kind": "sound", "unknown": [0, 11]}}, {"text": "They're as traditional as it gets!"}]}, "DemoMsg/Demo370_0:Demo370_0_Text011": {"contents": [{"control": {"kind": "sound", "unknown": [10, 0]}}, {"text": "I don't think she means traditional in that\nsense of the word."}]}, "DemoMsg/Demo370_0:Demo370_0_Text012": {"contents": [{"control": {"kind": "sound", "unknown": [7, 1]}}, {"control": {"kind": "animation", "name": "LaughL_00"}}, {"text": "A vow of undying tradition!"}]}, "DemoMsg/Demo370_0:Demo370_0_Text013": {"contents": [{"control": {"kind": "sound", "unknown": [7, 9]}}, {"text": "And a vow of guidelines compliance!"}]}, "DemoMsg/Demo370_0:Demo370_0_Text014": {"contents": [{"control": {"kind": "text_size", "percent": 125}}, {"control": {"kind": "sound", "unknown": [7, 10]}}, {"text": "I'll take that vow!"}, {"control": {"kind": "text_size", "percent": 100}}]}, "DemoMsg/Demo370_0:Demo370_0_Text015": {"contents": [{"control": {"kind": "sound2", "unknown": [10, 205]}}, {"control": {"kind": "sound", "unknown": [0, 1]}}, {"control": {"kind": "text_size", "percent": 80}}, {"text": "Well, those are the guidelines..."}, {"control": {"kind": "text_size", "percent": 100}}, {"control": {"kind": "pause", "length": "longer"}}, {"text": "\nI do."}]}, "DemoMsg/Demo370_0:Demo370_0_Text016": {"contents": [{"control": {"kind": "sound", "unknown": [7, 10]}}, {"text": "Think of future generations!"}]}, "DemoMsg/Demo370_0:Demo370_0_Text017": {"contents": [{"control": {"kind": "sound2", "unknown": [12, 205]}}, {"control": {"kind": "sound", "unknown": [0, 1]}}, {"text": "*"}, {"control": {"kind": "text_size", "percent": 125}}, {"text": "cough cough*"}, {"control": {"kind": "text_size", "percent": 100}}]}, "DemoMsg/Demo370_0:Demo370_0_Text018": {"contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "And so, dearly beloved,"}, {"control": {"kind": "pause", "length": "long"}}, {"text": " please help me\nin greeting this newly married couple."}]}, "DemoMsg/Demo370_0:Demo370_0_Text019": {"contents": [{"control": {"kind": "sound", "unknown": [7, 1]}}, {"text": "We are gathered here today to join\nHudson and Rhondson in matrimony."}]}, "DemoMsg/Demo370_0:Demo370_0_Text020": {"contents": [{"text": "..."}]}, "DemoMsg/Demo370_0:Demo370_0_Text021": {"contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "I wish this new couple nothing but the\nutmost happiness as they set out on this\njourney of marital bliss!"}]}, "DemoMsg/Demo370_0:Demo370_0_Text022": {"contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Before the eyes of these witnesses"}, {"control": {"kind": "pause", "length": "short"}}, {"text": "\n"}, {"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "and before those of Goddess Hylia..."}]}, "DemoMsg/Demo370_0:Demo370_0_Text023": {"contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Silence please, everyone."}, {"control": {"kind": "pause", "length": "long"}}, {"text": "\nWe shall now begin."}]}}