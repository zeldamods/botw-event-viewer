tents": [{"control": {"kind": "sound", "unknown": [2, 14]}}, {"text": "Hey! Captain Snoop!"}, {"control": {"kind": "pause", "length": "short"}}, {"text": " You want to get out\nof my personal space or what?"}, {"control": {"kind": "pause", "length": "short"}}, {"text": "\n\nEver heard of the concept of a PRIVATE\nconversation?"}]}, "EventFlowMsg/Npc_HatenoVillage020:talk07": {"attributes": "Npc_HatenoVillage006", "contents": [{"control": {"kind": "sound", "unknown": [8, 0]}}, {"text": "I'm trying to have a decent gossip here!"}, {"control": {"kind": "pause", "length": "long"}}, {"text": "\nAnd I don't need you! Now go away!"}]}, "EventFlowMsg/Npc_HatenoVillage020:talk12": {"attributes": "Npc_HatenoVillage020", "contents": [{"control": {"kind": "sound", "unknown": [11, 6]}}, {"text": "Hm?"}, {"control": {"kind": "pause", "length": "longer"}}, {"text": " I could've sworn someone just\ncalled out to me..."}]}, "EventFlowMsg/Npc_HatenoVillage020:talk13": {"attributes": "Npc_HatenoVillage020", "contents": [{"control": {"kind": "text_size", "percent": 80}}, {"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Urk..."}, {"control": {"kind": "text_size", "percent": 100}}]}, "EventFlowMsg/Npc_HatenoVillage020:talk14": {"attributes": "Npc_HatenoVillage020", "contents": [{"control": {"kind": "sound", "unknown": [0, 15]}}, {"text": "Excuse me, but I'm going to bed."}, {"control": {"kind": "pause", "length": "longer"}}, {"text": "\nCan you please get out of my way?"}]}, "EventFlowMsg/Npc_HatenoVillage020:talk15": {"attributes": "Npc_HatenoVillage020", "contents": [{"control": {"kind": "sound", "unknown": [0, 7]}}, {"text": "Some of us have things to do, y'know?"}, {"control": {"kind": "pause", "length": "longer"}}, {"text": "\nWould you mind leaving me alone?"}]}, "EventFlowMsg/Npc_HatenoVillage020:talk16": {"attributes": "Npc_HatenoVillage020", "contents": [{"control": {"kind": "sound", "unknown": [0, 13]}}, {"text": "...What?"}, {"control": {"kind": "pause", "length": "longer"}}, {"text": " Go hassle someone else."}]}, "EventFlowMsg/Npc_HatenoVillage020:talk17": {"attributes": "Npc_HatenoVillage020", "contents": [{"control": {"kind": "sound", "unknown": [0, 17]}}, {"text": "I have stuff I need to do now."}, {"control": {"kind": "pause", "length": "longer"}}, {"text": " \nCould you stop following me?"}]}}