{"EventFlowMsg/Npc_Goron020_2:0000": {"attributes": "", "contents": [{"text": "Umm..."}]}, "EventFlowMsg/Npc_Goron020_2:0001": {"attributes": "", "contents": [{"text": "Rudania?"}]}, "EventFlowMsg/Npc_Goron020_2:0002": {"attributes": "", "contents": [{"text": "Daruk's Protection?"}]}, "EventFlowMsg/Npc_Goron020_2:0003": {"attributes": "", "contents": [{"text": "Good-bye."}]}, "EventFlowMsg/Npc_Goron020_2:0004": {"attributes": "", "contents": [{"text": "You did your best!"}]}, "EventFlowMsg/Npc_Goron020_2:TerrorArrow": {"attributes": "", "contents": [{"text": "Waaaaaah, goro!"}]}, "EventFlowMsg/Npc_Goron020_2:TerrorBomb": {"attributes": "", "contents": [{"text": "What in the goro?!"}]}, "EventFlowMsg/Npc_Goron020_2:TerrorExplosion": {"attributes": "", "contents": [{"text": "Gyaaaah, goro!"}]}, "EventFlowMsg/Npc_Goron020_2:TerrorSword": {"attributes": "", "contents": [{"text": "Stop it, goro!"}]}, "EventFlowMsg/Npc_Goron020_2:near00": {"attributes": "Npc_Goron020_2", "contents": [{"text": "Hnkkk... Zzzz..."}]}, "EventFlowMsg/Npc_Goron020_2:talk00": {"attributes": "Npc_Goron020_2", "contents": [{"control": {"kind": "sound", "unknown": [0, 2]}}, {"text": "Heyo, good morning."}]}, "EventFlowMsg/Npc_Goron020_2:talk01": {"attributes": "Npc_Goron020_2", "contents": [{"control": {"kind": "sound", "unknown": [0, 2]}}, {"text": "Ah, it's you."}]}, "EventFlowMsg/Npc_Goron020_2:talk02": {"attributes": "Npc_Goron020_2", "contents": [{"control": {"kind": "sound", "unknown": [0, 2]}}, {"text": "Good evening."}]}, "EventFlowMsg/Npc_Goron020_2:talk03": {"attributes": "Npc_Goron020_2", "contents": [{"control": {"kind": "sound", "unknown": [0, 6]}}, {"text": "Huh?"}]}, "EventFlowMsg/Npc_Goron020_2:talk06": {"attributes": "Npc_Goron020_2", "contents": [{"control": {"kind": "sound", "unknown": [1, 6]}}, {"text": "Anything else you wanna ask about,\ngoro?"}, {"control": {"kind": "choice", "choice_labels": [0, 1, 2, 3], "selected_index": 0, "cancel_index": 3, "unknown": 10}}]}, "EventFlowMsg/Npc_Goron020_2:talk07": {"attributes": "Npc_Goron020_2", "contents": [{"control": {"kind": "sound", "unknown": [0, 1]}}, {"text": "HNKKKGH"}, {"control": {"kind": "text_size", "percent": 125}}, {"text": "..."}, {"control": {"kind": "text_size", "percent": 100}}]}, "EventFlowMsg/Npc_Goron020_2:talk08": {"attributes": "Npc_Goron020_2", "contents": [{"control": {"kind": "sound", "unknown": [0, 1]}}, {"control": {"kind": "text_size", "percent": 125}}, {"text": "GRUUUGH..."}, {"control": {"kind": "text_size", "percent": 100}}]}, "EventFlowMsg/Npc_Goron020_2:talk09": {"attributes": "Npc_Goron020_2", "contents": [{"control": {"kind": "sound", "unknown": [0, 1]}}, {"control": {"kind": "text_size", "percent": 125}}, {"text": "GRARGHHH..."}, {"control": {"kind": "text_size", "percent": 100}}]}, "EventFlowMsg/Npc_Goron020_2:talk10": {"attributes": "Npc_Goron020_2", "contents": [{"control": {"kind": "sound", "unknown": [0, 1]}}, {"text": "*"}, {"control": {"kind": "text_size", "percent": 125}}, {"text": "GURRRGLE*"}, {"control": {"kind": "text_size", "percent": 100}}]}, "EventFlowMsg/Npc_Goron020_2:talk11": {"attributes": "Npc_Goron020_2", "contents": [{"control": {"kind": "sound", "unknown": [1, 6]}}, {"text": "You need somethin', goro?"}, {"control": {"kind": "choice", "choice_labels": [0, 1, 2, 3], "selected_index": 0, "cancel_index": 3, "unknown": 10}}]}, "EventFlowMsg/Npc_Goron020_2:talk12": {"attributes": "Npc_Goron020_2", "contents": [{"control": {"kind": "sound", "unknown": [0, 4]}}, {"text": "See ya later, goro."}]}, "EventFlowMsg/Npc_Goron020_2:talk13": {"attributes": "Npc_Goron020_2", "contents": [{"control": {"kind": "sound", "unknown": [0, 19]}}, {"text": "So you figured it out, goro..."}]}, "EventFlowMsg/Npc_Goron020_2:talk14": {"attributes": "Npc_Goron020_2", "contents": [{"control": {"kind": "sound", "unknown": [0, 4]}}, {"text": "Ever since "}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "Rudania "}, {"control": {"kind": "reset_colour"}}, {"text": "settled down there,"}, {"control": {"kind": "pause", "length": "short"}}, {"text": "\nit hasn't moved at all, goro. "}, {"control": {"kind": "pause", "length": "short"}}, {"text": "To think\nwe actually fought that thing..."}]}, "EventFlowMsg/Npc_Goron020_2:talk15": {"attributes": "Npc_Goron020_2", "contents": [{"control": {"kind": "sound", "unknown": [0, 4]}}, {"text": "Ever since we fought "}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "Rudania"}, {"control": {"kind": "reset_colour"}}, {"text": ", I have this\ncrazy feeling..."}, {"control": {"kind": "pause", "length": "short"}}, {"text": "that my ancestor is always\nwith me. "}, {"control": {"kind": "pause", "length": "short"}}, {"text": "Strange, goro."}]}, "EventFlowMsg/Npc_Goron020_2:talk16": {"attributes": "Npc_Goron020_2", "contents": [{"control": {"kind": "sound", "unknown": [1, 0]}}, {"text": "So long as I have "}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "Daruk's Protection"}, {"control": {"kind": "reset_colour"}}, {"text": ",\nI have nothing to fear!"}]}, "EventFlowMsg/Npc_Goron020_2:talk17": {"attributes": "Npc_Goron020_2", "contents": [{"control": {"kind": "sound", "unknown": [1, 35]}}, {"text": "Hehe, we're on our way to a secret spot.\n"}, {"control": {"kind": "pause", "length": "short"}}, {"text": "If you wanna talk, just wait until we\nget there, goro."}]}, "EventFlowMsg/Npc_Goron020_2:talk18": {"attributes": "Npc_Goron020_2", "contents": [{"control": {"kind": "sound", "unknown": [1, 0]}}, {"text": "I'm goin' for a walk today, goro."}, {"control": {"kind": "pause", "length": "short"}}, {"text": "\nHey, wanna come along?"}]}, "EventFlowMsg/Npc_Goron020_2:talk19": {"attributes": "Npc_Goron020_2", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Welcome to my secret hideout, goro!"}]}, "EventFlowMsg/Npc_Goron020_2:talk20": {"attributes": "Npc_Goron020_2", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "To think we actually fought that thing..."}, {"control": {"kind": "pause", "length": "short"}}, {"text": "\nAmazing, goro.\n\nAnd I wasn't even scared of it...\nuntil just now."}]}, "EventFlowMsg/Npc_Goron020_2:talk21": {"attributes": "Npc_Goron020_2", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Mmm, time to get some shut-eye!"}, {"control": {"kind": "pause", "length": "longer"}}, {"text": "\n...Don't you ever sleep, goro?"}]}, "EventFlowMsg/Npc_Goron020_2:talk22": {"attributes": "Npc_Goron020_2", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "I'm sleepy, and now my whole body is\nall warm and cozy..."}, {"control": {"kind": "pause", "length": "short"}}, {"text": " Come back later if\nyou wanna chat."}]}, "EventFlowMsg/Npc_Goron020_2:talk23": {"attributes": "Npc_Goron020_2", "contents": [{"control": {"kind": "sound", "unknown": [1, 0]}}, {"text": "The "}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "Boss"}, {"control": {"kind": "reset_colour"}}, {"text": " said his back is all better, goro!"}, {"control": {"kind": "pause", "length": "short"}}, {"text": "\nOh, and "}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "Rudania "}, {"control": {"kind": "reset_colour"}}, {"text": "stopped rampaging!\n\nI feel like...I can finally take it easy, goro."}]}, "EventFlowMsg/Npc_Goron020_2:talk24": {"attributes": "Npc_Goron020_2", "contents": [{"control": {"kind": "sound", "unknown": [1, 4]}}, {"text": "Now that I defeated "}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "Rudania"}, {"control": {"kind": "reset_colour"}}, {"text": ","}, {"control": {"kind": "pause", "length": "short"}}, {"text": " I guess\nthat means I've proven myself! Right?\n\nNow maybe I can get the respect\nI deserve..."}, {"control": {"kind": "pause", "length": "short"}}, {"text": "goro.\n\nAnd my ancestor... "}, {"control": {"kind": "pause", "length": "short"}}, {"text": "I'll have to do my\nbest to keep honoring Lord Daruk's\nlegacy and all that!"}]}, "EventFlowMsg/Npc_Goron020_2:talk25": {"attributes": "Npc_Goron020_2", "contents": [{"control": {"kind": "sound", "unknown": [0, 11]}}, {"text": "Take a look over there, goro!\n\n\nSee? "}, {"control": {"kind": "pause", "length": "longer"}}, {"text": "This is the best place to get a nice\nview of "}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "Rudania"}, {"control": {"kind": "reset_colour"}}, {"text": "!"}]}, "EventFlowMsg/Npc_Goron020_2:talk26": {"attributes": "Npc_Goron020_2", "contents": [{"control": {"kind": "sound", "unknown": [0, 6]}}, {"text": "I..."}, {"control": {"kind": "pause", "length": "short"}}, {"text": "I did well, didn't I? Goro?"}, {"control": {"kind": "single_choice", "label": 4}}, {"text": "\n\n\n"}, {"control": {"kind": "sound", "unknown": [1, 4]}}, {"text": "Yeah... "}, {"control": {"kind": "pause", "length": "short"}}, {"text": "Yeah, I thought so!"}, {"control": {"kind": "pause", "length": "longer"}}, {"text": "\nThanks, brother!\n\n"}, {"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "This spot is our little secret, OK?"}, {"control": {"kind": "pause", "length": "short"}}, {"text": "\nJust between you and me.\n\nAnytime you want a great view of\n"}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "Rudania"}, {"control": {"kind": "reset_colour"}}, {"text": ","}, {"control": {"kind": "pause", "length": "short"}}, {"text": " just come here, goro!"}]}, "EventFlowMsg/Npc_Goron020_2:talk27": {"attributes": "Npc_Goron020_2", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Hey!"}]}}