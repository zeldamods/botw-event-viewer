{"EventFlowMsg/Npc_RitoHatago002:0000": {"attributes": "", "contents": [{"text": "Yes, I do."}]}, "EventFlowMsg/Npc_RitoHatago002:0001": {"attributes": "", "contents": [{"text": "No, I don't."}]}, "EventFlowMsg/Npc_RitoHatago002:0002": {"attributes": "", "contents": [{"text": "Where am I?"}]}, "EventFlowMsg/Npc_RitoHatago002:0003": {"attributes": "", "contents": [{"text": "Bye."}]}, "EventFlowMsg/Npc_RitoHatago002:talk_00": {"attributes": "Npc_RitoHatago002", "contents": [{"control": {"kind": "sound", "unknown": [7, 0]}}, {"text": "Good morning."}, {"control": {"kind": "pause", "length": "long"}}, {"text": "\nDo you need a bed?"}, {"control": {"kind": "choice", "choice_labels": [0, 1, 2, 3], "selected_index": 0, "cancel_index": 3, "unknown": 10}}]}, "EventFlowMsg/Npc_RitoHatago002:talk_01": {"attributes": "Npc_RitoHatago002", "contents": [{"control": {"kind": "sound", "unknown": [7, 0]}}, {"text": "Hello. "}, {"control": {"kind": "pause", "length": "long"}}, {"text": "Do you need a bed?"}, {"control": {"kind": "choice", "choice_labels": [0, 1, 2, 3], "selected_index": 0, "cancel_index": 3, "unknown": 10}}]}, "EventFlowMsg/Npc_RitoHatago002:talk_02": {"attributes": "Npc_RitoHatago002", "contents": [{"control": {"kind": "sound", "unknown": [4, 11]}}, {"text": "You're coming in pretty late. You need a\nbed?"}, {"control": {"kind": "choice", "choice_labels": [0, 1, 2, 3], "selected_index": 0, "cancel_index": 3, "unknown": 10}}]}, "EventFlowMsg/Npc_RitoHatago002:talk_04": {"attributes": "Npc_RitoHatago002", "contents": [{"control": {"kind": "sound", "unknown": [1, 4]}}, {"text": "If you'd like to use the stable,"}, {"control": {"kind": "pause", "length": "short"}}, {"text": " speak to\nthe master at the counter."}]}, "EventFlowMsg/Npc_RitoHatago002:talk_08": {"attributes": "Npc_RitoHatago002", "contents": [{"control": {"kind": "sound", "unknown": [7, 4]}}, {"text": "Ah, a customer. Need a bed?"}, {"control": {"kind": "choice", "choice_labels": [0, 1, 2, 3], "selected_index": 0, "cancel_index": 3, "unknown": 10}}]}, "EventFlowMsg/Npc_RitoHatago002:talk_09": {"attributes": "Npc_RitoHatago002", "contents": [{"control": {"kind": "sound", "unknown": [0, 12]}}, {"text": "Oh, I see..."}]}, "EventFlowMsg/Npc_RitoHatago002:talk_10": {"attributes": "Npc_RitoHatago002", "contents": [{"control": {"kind": "sound", "unknown": [1, 4]}}, {"text": "Ha!"}, {"control": {"kind": "pause", "length": "short"}}, {"text": " Are you lost or something?"}, {"control": {"kind": "pause", "length": "long"}}, {"text": "\nThis is "}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "Rito Stable"}, {"control": {"kind": "reset_colour"}}, {"text": ".\n\n"}, {"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "We can help you with horse affairs\nat the counter outside,"}, {"control": {"kind": "pause", "length": "short"}}, {"text": " or you can stay\nthe night at the inn inside.\nIf you're wondering why we called this\nplace "}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "Rito Stable"}, {"control": {"kind": "reset_colour"}}, {"text": ", well..."}]}, "EventFlowMsg/Npc_RitoHatago002:talk_11": {"attributes": "Npc_RitoHatago002", "contents": [{"control": {"kind": "sound", "unknown": [0, 12]}}, {"text": "I see... "}, {"control": {"kind": "pause", "length": "short"}}, {"text": "Good-bye."}]}, "EventFlowMsg/Npc_RitoHatago002:talk_12": {"attributes": "Npc_RitoHatago002", "contents": [{"control": {"kind": "sound", "unknown": [1, 9]}}, {"text": "We took our name from the "}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "Rito "}, {"control": {"kind": "reset_colour"}}, {"text": "people\nwho live there."}]}, "EventFlowMsg/Npc_RitoHatago002:talk_13": {"attributes": "Npc_RitoHatago002", "contents": [{"control": {"kind": "sound", "unknown": [7, 0]}}, {"text": "Good morning."}, {"control": {"kind": "pause", "length": "short"}}, {"text": "\nIt looks like we're going to have beautiful\nweather today."}]}, "EventFlowMsg/Npc_RitoHatago002:talk_14": {"attributes": "Npc_RitoHatago002", "contents": [{"control": {"kind": "sound", "unknown": [6, 0]}}, {"text": "Good morning."}, {"control": {"kind": "pause", "length": "short"}}, {"text": "\nIt's too bad the weather's like this\ntoday..."}]}, "EventFlowMsg/Npc_RitoHatago002:talk_15": {"attributes": "Npc_RitoHatago002", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "You see that tower over there?"}]}, "EventFlowMsg/Npc_RitoHatago002:talk_16": {"attributes": "Npc_RitoHatago002", "contents": [{"control": {"kind": "sound", "unknown": [1, 4]}}, {"text": "This is "}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "Rito Stable"}, {"control": {"kind": "reset_colour"}}, {"text": ".\n\n\nWe can help you with horse affairs\nat the counter outside,"}, {"control": {"kind": "pause", "length": "short"}}, {"text": " or you can stay\nthe night at the inn inside.\nThe "}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "Rito"}, {"control": {"kind": "reset_colour"}}, {"text": " live in the village just west of\nhere.\n\nHence the name "}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "Rito Stable"}, {"control": {"kind": "reset_colour"}}, {"text": ". "}]}}