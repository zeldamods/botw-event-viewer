{"EventFlowMsg/Npc_HatenoVillage031:0000": {"attributes": "", "contents": [{"text": "What are you doing?"}]}, "EventFlowMsg/Npc_HatenoVillage031:0001": {"attributes": "", "contents": [{"text": "Who are you?"}]}, "EventFlowMsg/Npc_HatenoVillage031:0002": {"attributes": "", "contents": [{"text": "Bye."}]}, "EventFlowMsg/Npc_HatenoVillage031:0003": {"attributes": "", "contents": [{"text": "Why?"}]}, "EventFlowMsg/Npc_HatenoVillage031:0004": {"attributes": "", "contents": [{"text": "I'll buy this place!"}]}, "EventFlowMsg/Npc_HatenoVillage031:0005": {"attributes": "", "contents": [{"text": "That sounds tough."}]}, "EventFlowMsg/Npc_HatenoVillage031:0006": {"attributes": "", "contents": [{"text": "Why?"}]}, "EventFlowMsg/Npc_HatenoVillage031:0007": {"attributes": "", "contents": [{"text": "Why?"}]}, "EventFlowMsg/Npc_HatenoVillage031:0008": {"attributes": "", "contents": [{"text": "Oh."}]}, "EventFlowMsg/Npc_HatenoVillage031:0009": {"attributes": "", "contents": [{"text": "Yes."}]}, "EventFlowMsg/Npc_HatenoVillage031:0010": {"attributes": "", "contents": [{"text": "Uh...come again?"}]}, "EventFlowMsg/Npc_HatenoVillage031:0011": {"attributes": "", "contents": [{"text": "Good."}]}, "EventFlowMsg/Npc_HatenoVillage031:0012": {"attributes": "", "contents": [{"text": "Yeah."}]}, "EventFlowMsg/Npc_HatenoVillage031:0013": {"attributes": "", "contents": [{"text": "What?"}]}, "EventFlowMsg/Npc_HatenoVillage031:0014": {"attributes": "", "contents": [{"text": "Transferred..."}]}, "EventFlowMsg/Npc_HatenoVillage031:0015": {"attributes": "", "contents": [{"text": "That's great!"}]}, "EventFlowMsg/Npc_HatenoVillage031:0016": {"attributes": "", "contents": [{"text": "Where will you go?"}]}, "EventFlowMsg/Npc_HatenoVillage031:0017": {"attributes": "", "contents": [{"text": "That's pretty far."}]}, "EventFlowMsg/Npc_HatenoVillage031:0018": {"attributes": "", "contents": [{"text": "Good luck!"}]}, "EventFlowMsg/Npc_HatenoVillage031:0019": {"attributes": "", "contents": [{"text": "Right."}]}, "EventFlowMsg/Npc_HatenoVillage031:0021": {"attributes": "", "contents": [{"text": "I'm Link."}]}, "EventFlowMsg/Npc_HatenoVillage031:0022": {"attributes": "", "contents": [{"text": "Son?"}]}, "EventFlowMsg/Npc_HatenoVillage031:0023": {"attributes": "", "contents": [{"text": "That's too bad."}]}, "EventFlowMsg/Npc_HatenoVillage031:near00": {"attributes": "Npc_HatenoVillage031", "contents": [{"text": "Yo!"}]}, "EventFlowMsg/Npc_HatenoVillage031:near01": {"attributes": "Npc_HatenoVillage031", "contents": [{"text": "Ho!"}]}, "EventFlowMsg/Npc_HatenoVillage031:near02": {"attributes": "Npc_HatenoVillage031", "contents": [{"text": "Hmph!"}]}, "EventFlowMsg/Npc_HatenoVillage031:near03": {"attributes": "Npc_HatenoVillage031", "contents": [{"text": "Yah!"}]}, "EventFlowMsg/Npc_HatenoVillage031:near04": {"attributes": "Npc_HatenoVillage031", "contents": [{"text": "Hahhhh..."}]}, "EventFlowMsg/Npc_HatenoVillage031:talk00": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 4]}}, {"text": "Hey."}, {"control": {"kind": "choice", "choice_labels": [0, 1, 2], "selected_index": 0, "cancel_index": 2, "unknown": 8}}]}, "EventFlowMsg/Npc_HatenoVillage031:talk01": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 19]}}, {"text": "Yeah?"}, {"control": {"kind": "choice", "choice_labels": [0, 1, 2], "selected_index": 0, "cancel_index": 2, "unknown": 8}}]}, "EventFlowMsg/Npc_HatenoVillage031:talk02": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Hahhh..."}, {"control": {"kind": "choice", "choice_labels": [0, 1, 2], "selected_index": 0, "cancel_index": 2, "unknown": 8}}]}, "EventFlowMsg/Npc_HatenoVillage031:talk03": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 18]}}, {"text": "...Hahhh..."}, {"control": {"kind": "choice", "choice_labels": [0, 1, 2], "selected_index": 0, "cancel_index": 2, "unknown": 8}}]}, "EventFlowMsg/Npc_HatenoVillage031:talk04": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "The "}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "blue flame"}, {"control": {"kind": "reset_colour"}}, {"text": ", huh?"}, {"control": {"kind": "choice", "choice_labels": [12, 2], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_HatenoVillage031:talk06": {"attributes": "Npc_UMiiVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Farewell then."}]}, "EventFlowMsg/Npc_HatenoVillage031:talk07": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 6]}}, {"text": "Oh."}, {"control": {"kind": "choice", "choice_labels": [0, 1, 2], "selected_index": 0, "cancel_index": 2, "unknown": 8}}]}, "EventFlowMsg/Npc_HatenoVillage031:talk08": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Hello."}, {"control": {"kind": "choice", "choice_labels": [0, 1, 2], "selected_index": 0, "cancel_index": 2, "unknown": 8}}]}, "EventFlowMsg/Npc_HatenoVillage031:talk09": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "I'm in the middle of moving here."}, {"control": {"kind": "pause", "length": "longer"}}, {"text": " \nCome talk when the work dies down."}]}, "EventFlowMsg/Npc_HatenoVillage031:talk10": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "I'm a carpenter for "}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "Bolson Construction"}, {"control": {"kind": "reset_colour"}}, {"text": ".\n"}, {"control": {"kind": "pause", "length": "short"}}, {"text": "Name's Hudson."}, {"control": {"kind": "choice", "choice_labels": [0, 2], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_HatenoVillage031:talk11": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "I'm knocking this place down."}, {"control": {"kind": "choice", "choice_labels": [3, 4, 5, 2], "selected_index": 0, "cancel_index": 3, "unknown": 10}}]}, "EventFlowMsg/Npc_HatenoVillage031:talk12": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "That's right. It's tough."}]}, "EventFlowMsg/Npc_HatenoVillage031:talk13": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "'Cause no one's living here."}, {"control": {"kind": "choice", "choice_labels": [6, 2], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_HatenoVillage031:talk14": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Can't help you. Ask the "}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "boss"}, {"control": {"kind": "reset_colour"}}, {"text": "."}]}, "EventFlowMsg/Npc_HatenoVillage031:talk15": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "The owner of this house took off."}, {"control": {"kind": "choice", "choice_labels": [7, 2], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_HatenoVillage031:talk16": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 16]}}, {"text": "Don't know."}, {"control": {"kind": "pause", "length": "short"}}, {"text": " Maybe skipped town without\npaying the bills?"}, {"control": {"kind": "choice", "choice_labels": [8, 2], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_HatenoVillage031:talk17": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Answered all your questions?"}, {"control": {"kind": "choice", "choice_labels": [9, 2], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_HatenoVillage031:talk18": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Good."}, {"control": {"kind": "pause", "length": "short"}}, {"text": " A lot has happened since the\n"}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "Great Calamity"}, {"control": {"kind": "reset_colour"}}, {"text": "."}, {"control": {"kind": "pause", "length": "short"}}, {"text": " Tough to keep track of\nit all."}, {"control": {"kind": "choice", "choice_labels": [19, 2], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_HatenoVillage031:talk19": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "For the first time since then, we're finally\ngetting back on our feet."}, {"control": {"kind": "choice", "choice_labels": [11, 2], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_HatenoVillage031:talk20": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 9]}}, {"text": "So best of luck to both of us."}, {"control": {"kind": "choice", "choice_labels": [12, 2], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_HatenoVillage031:talk21": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Yeah."}]}, "EventFlowMsg/Npc_HatenoVillage031:talk22": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Taking a break."}]}, "EventFlowMsg/Npc_HatenoVillage031:talk23": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 6]}}, {"text": "By the way..."}, {"control": {"kind": "pause", "length": "longer"}}, {"text": " Did you"}, {"control": {"kind": "pause", "length": "short"}}, {"text": " buy that house?"}, {"control": {"kind": "choice", "choice_labels": [9, 2], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_HatenoVillage031:talk24": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Impressive. You're just a pup."}, {"control": {"kind": "choice", "choice_labels": [10, 2], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_HatenoVillage031:talk25": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Don't worry about it."}, {"control": {"kind": "pause", "length": "long"}}, {"text": " Anyway..."}, {"control": {"kind": "choice", "choice_labels": [13, 2], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_HatenoVillage031:talk26": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Looks like I've gotta head out and leave\nthis place soon."}, {"control": {"kind": "choice", "choice_labels": [3, 2], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_HatenoVillage031:talk27": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "I've been transferred."}, {"control": {"kind": "choice", "choice_labels": [14, 2], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_HatenoVillage031:talk28": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Mighta heard, but Bolson Construction\nis planning to expand big-time."}, {"control": {"kind": "choice", "choice_labels": [15, 2], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_HatenoVillage031:talk29": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Yeah, big expansion."}, {"control": {"kind": "choice", "choice_labels": [16, 2], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_HatenoVillage031:talk30": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Zora's Domain is north of here."}, {"control": {"kind": "pause", "length": "long"}}, {"text": "\nWe're going north of that, though.\nAn area called the "}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "Akkala region"}, {"control": {"kind": "reset_colour"}}, {"text": "."}, {"control": {"kind": "choice", "choice_labels": [17, 2], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_HatenoVillage031:talk31": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Yeah..."}, {"control": {"kind": "pause", "length": "longer"}}, {"text": "well."}, {"control": {"kind": "choice", "choice_labels": [18, 2], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_HatenoVillage031:talk32": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "OK then."}]}, "EventFlowMsg/Npc_HatenoVillage031:talk34": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 1]}}, {"text": "By the way..."}, {"control": {"kind": "choice", "choice_labels": [13, 2], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_HatenoVillage031:talk35": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 6]}}, {"text": "Are you gonna buy that house?"}, {"control": {"kind": "choice", "choice_labels": [12, 2], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_HatenoVillage031:talk36": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [1, 9]}}, {"text": "You're pretty studly, aren't you?"}, {"control": {"kind": "choice", "choice_labels": [10, 2], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_HatenoVillage031:talk37": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "You'd be a great hire for our company..."}, {"control": {"kind": "pause", "length": "longer"}}, {"text": "\nWhat's your name?"}, {"control": {"kind": "choice", "choice_labels": [21, 2], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_HatenoVillage031:talk38": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Oh... "}, {"control": {"kind": "pause", "length": "short"}}, {"text": "Well, too bad. Can't hire you."}, {"control": {"kind": "choice", "choice_labels": [6, 2], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_HatenoVillage031:talk39": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "'Cause your name doesn't end in \""}, {"control": {"kind": "set_colour", "colour": "red"}}, {"text": "son"}, {"control": {"kind": "reset_colour"}}, {"text": ".\""}, {"control": {"kind": "choice", "choice_labels": [22, 2], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_HatenoVillage031:talk40": {"attributes": "Npc_HatenoVillage031", "contents": [{"text": "The boss, Bolson, has a strict hiring\nrule: "}, {"control": {"kind": "set_colour", "colour": "red"}}, {"text": "need a name ending in \"son"}, {"control": {"kind": "reset_colour"}}, {"text": "."}, {"control": {"kind": "set_colour", "colour": "red"}}, {"text": "\""}, {"control": {"kind": "reset_colour"}}, {"text": "\nNo exceptions."}, {"control": {"kind": "choice", "choice_labels": [23, 2], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_HatenoVillage031:talk41": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "\"Builds team unity,\" he says.\n\n\nAnyway...\n\n\nYou got some stuff to do, right?"}, {"control": {"kind": "pause", "length": "longer"}}, {"text": "\nShould get going, then."}]}, "EventFlowMsg/Npc_HatenoVillage031:talk42": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Well... "}, {"control": {"kind": "pause", "length": "short"}}, {"text": "No time like the present, right?\nI should be getting on my way...\n\nIf you'd like, you should come to the\n"}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "Akkala region"}, {"control": {"kind": "reset_colour"}}, {"control": {"kind": "pause", "length": "short"}}, {"text": " and say hello.\n\nWell... "}, {"control": {"kind": "pause", "length": "short"}}, {"text": "Take care of yourself."}]}, "EventFlowMsg/Npc_HatenoVillage031:talk43": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "I'm off to my usual night spot."}]}, "EventFlowMsg/Npc_HatenoVillage031:talk44": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "I'm on lookout."}]}, "EventFlowMsg/Npc_HatenoVillage031:talk45": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "How goes it?"}, {"control": {"kind": "pause", "length": "longer"}}, {"text": " How's the "}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "boss"}, {"control": {"kind": "reset_colour"}}, {"text": "?\nDoing well, I hope?"}]}, "EventFlowMsg/Npc_HatenoVillage031:talk46": {"attributes": "Npc_HatenoVillage031", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Pretty sure that belongs to the\n"}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "Hateno Ancient Tech Lab"}, {"control": {"kind": "reset_colour"}}, {"text": "."}, {"control": {"kind": "pause", "length": "longer"}}, {"text": "\nBetter hurry up and take it back there."}]}}