{"EventFlowMsg/Npc_HatenoVillage019:0000": {"attributes": "", "contents": [{"text": "You like this corner?"}]}, "EventFlowMsg/Npc_HatenoVillage019:0001": {"attributes": "", "contents": [{"text": "I'm here to sell."}]}, "EventFlowMsg/Npc_HatenoVillage019:0002": {"attributes": "", "contents": [{"text": "What is this place?"}]}, "EventFlowMsg/Npc_HatenoVillage019:0003": {"attributes": "", "contents": [{"text": "Good-bye."}]}, "EventFlowMsg/Npc_HatenoVillage019:0004": {"attributes": "", "contents": [{"text": "I'll buy."}]}, "EventFlowMsg/Npc_HatenoVillage019:5500": {"attributes": "", "contents": [{"text": "Yes, please."}]}, "EventFlowMsg/Npc_HatenoVillage019:5501": {"attributes": "", "contents": [{"text": "No, thanks."}]}, "EventFlowMsg/Npc_HatenoVillage019:5502": {"attributes": "", "contents": [{"text": "Buy."}]}, "EventFlowMsg/Npc_HatenoVillage019:5503": {"attributes": "", "contents": [{"text": "Never mind."}]}, "EventFlowMsg/Npc_HatenoVillage019:5508": {"attributes": "", "contents": [{"text": "All of them."}]}, "EventFlowMsg/Npc_HatenoVillage019:Com_Talk_00": {"attributes": "", "contents": [{"text": "Please go ahead and sell whatever\nyou desire..."}]}, "EventFlowMsg/Npc_HatenoVillage019:Com_Talk_01": {"attributes": "", "contents": [{"text": "For that...I can give you "}, {"control": {"kind": "set_colour", "colour": "red"}}, {"control": {"kind": "variable", "variable_kind": 18, "name": "ShopTradePrice"}}, {"text": " rupees"}, {"control": {"kind": "reset_colour"}}, {"text": ".\nIs that OK?"}, {"control": {"kind": "choice", "choice_labels": [5500, 5501], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_HatenoVillage019:Com_Talk_02": {"attributes": "", "contents": [{"control": {"kind": "sound2", "unknown": [9, 205]}}, {"text": "Thank you very much! Would you like to\nsell anything else?"}]}, "EventFlowMsg/Npc_HatenoVillage019:Com_Talk_02b": {"attributes": "", "contents": [{"control": {"kind": "sound2", "unknown": [9, 205]}}, {"text": "Much obliged! Anything else?"}]}, "EventFlowMsg/Npc_HatenoVillage019:Com_Talk_02c": {"attributes": "", "contents": [{"control": {"kind": "sound2", "unknown": [9, 205]}}, {"text": "Well thanks so much! Anything else?"}]}, "EventFlowMsg/Npc_HatenoVillage019:Com_Talk_02d": {"attributes": "", "contents": [{"control": {"kind": "sound2", "unknown": [9, 205]}}, {"text": "Pleasure doing business with ya!\nAnything else you might like to sell?"}]}, "EventFlowMsg/Npc_HatenoVillage019:Com_Talk_04": {"attributes": "", "contents": [{"text": "OK then!"}]}, "EventFlowMsg/Npc_HatenoVillage019:Com_Talk_05": {"attributes": "", "contents": [{"text": "We'll buy anything!"}]}, "EventFlowMsg/Npc_HatenoVillage019:Com_Talk_05b": {"attributes": "", "contents": [{"text": "No rush."}]}, "EventFlowMsg/Npc_HatenoVillage019:Com_Talk_05c": {"attributes": "", "contents": [{"text": "Take your time..."}]}, "EventFlowMsg/Npc_HatenoVillage019:Com_Talk_05d": {"attributes": "", "contents": [{"text": "What will you sell?"}]}, "EventFlowMsg/Npc_HatenoVillage019:Com_Talk_06": {"attributes": "", "contents": [{"text": "Which one would you like to buy?"}]}, "EventFlowMsg/Npc_HatenoVillage019:Com_Talk_07": {"attributes": "", "contents": [{"control": {"kind": "sound", "unknown": [5, 0]}}, {"text": "So sorry\u2014that item is currently sold out!"}]}, "EventFlowMsg/Npc_HatenoVillage019:Com_Talk_08": {"attributes": "", "contents": [{"control": {"kind": "sound", "unknown": [2, 0]}}, {"text": "Sir, you don't have quite enough for\nthis purchase."}]}, "EventFlowMsg/Npc_HatenoVillage019:Com_Talk_09": {"attributes": "", "contents": [{"control": {"kind": "sound", "unknown": [5, 0]}}, {"text": "Sir, it seems that you can't carry any\nmore items."}]}, "EventFlowMsg/Npc_HatenoVillage019:Com_Talk_10": {"attributes": "", "contents": [{"control": {"kind": "set_colour", "colour": "blue"}}, {"control": {"kind": "sound", "unknown": [5, 0]}}, {"control": {"kind": "reset_colour"}}, {"text": "Would you like to buy that, then?"}, {"control"