{"EventFlowMsg/ArmorProcessingEventTag:0000": {"attributes": "", "contents": [{"text": "Sure."}]}, "EventFlowMsg/ArmorProcessingEventTag:0001": {"attributes": "", "contents": [{"text": "I'll pass."}]}, "EventFlowMsg/ArmorProcessingEventTag:0005": {"attributes": "", "contents": [{"text": "I'll keep my rupees."}]}, "EventFlowMsg/ArmorProcessingEventTag:0014": {"attributes": "", "contents": [{"text": "Here you go."}]}, "EventFlowMsg/ArmorProcessingEventTag:Talk00": {"attributes": "GreatFairyBeforeRevival", "contents": [{"control": {"kind": "sound2", "unknown": [0, 205]}}, {"control": {"kind": "text_size", "percent": 80}}, {"text": "Boy..."}, {"control": {"kind": "text_size", "percent": 100}}, {"text": "\n\n\nSweet boy...\n\n\nPlease..."}, {"control": {"kind": "pause", "length": "longer"}}, {"text": " Listen to my story..."}, {"control": {"kind": "choice", "choice_labels": [0, 1], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/ArmorProcessingEventTag:Talk01": {"attributes": "GreatFairyBeforeRevival", "contents": [{"text": "Now... "}, {"control": {"kind": "pause", "length": "short"}}, {"text": "You have rupees for me, right?"}, {"control": {"kind": "pause", "length": "short"}}, {"text": "\nHand them over. Quickly!"}]}, "EventFlowMsg/ArmorProcessingEventTag:Talk02": {"attributes": "GreatFairyBeforeRevival", "contents": [{"control": {"kind": "sound2", "unknown": [10, 205]}}, {"text": "That is truly tragic..."}]}, "EventFlowMsg/ArmorProcessingEventTag:Talk03": {"attributes": "GreatFairyBeforeRevival", "contents": [{"control": {"kind": "sound2", "unknown": [0, 205]}}, {"text": "Ah..."}, {"control": {"kind": "pause", "length": "short"}}, {"text": " You've returned."}]}, "EventFlowMsg/ArmorProcessingEventTag:Talk03_01": {"attributes": "GreatFairyBeforeRevival", "contents": [{"text": "Now... "}, {"control": {"kind": "pause", "length": "short"}}, {"text": "If you have rupees,"}, {"control": {"kind": "pause", "length": "short"}}, {"text": " hand them\nover to me. Quickly!"}]}, "EventFlowMsg/ArmorProcessingEventTag:Talk03_01B": {"attributes": "GreatFairyBeforeRevival", "contents": [{"control": {"kind": "sound2", "unknown": [2, 205]}}, {"text": "In order for me to regain my power,"}, {"control": {"kind": "pause", "length": "short"}}, {"text": "\nI need "}, {"control": {"kind": "set_colour", "colour": "red"}}, {"control": {"kind": "variable", "variable_kind": 17, "name": "AmountRupeeForNextRevivalFountain"}}, {"text": " rupees"}, {"control": {"kind": "reset_colour"}}, {"text": "."}, {"control": {"kind": "choice", "choice_labels": [14, 5], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/ArmorProcessingEventTag:Talk03_01C": {"attributes": "GreatFairyBeforeRevival", "contents": [{"text": "Oh? I feel the power of a new sister..."}, {"control": {"kind": "pause", "length": "longer"}}, {"text": "\nQuickly,"}, {"control": {"kind": "pause", "length": "short"}}, {"text": " free me as well!"}]}, "EventFlowMsg/ArmorProcessingEventTag:Talk03_05B": {"attributes": "GreatFairyBeforeRevival", "contents": [{"control": {"kind": "sound2", "unknown": [2, 205]}}, {"text": "In order for me to regain my power,"}, {"control": {"kind": "pause", "length": "short"}}, {"text": "\nI need "}, {"control": {"kind": "set_colour", "colour": "red"}}, {"text": "1,000 rupees"}, {"control": {"kind": "reset_colour"}}, {"text": "."}, {"control": {"kind": "choice", "choice_labels": [14, 5], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/ArmorProcessingEventTag:Talk03_m": {"attributes": "GreatFairyBeforeRevival", "contents": [{"control": {"kind": "sound2", "unknown": [0, 205]}}, {"text": "Well..."}, {"control": {"kind": "pause", "length": "short"}}, {"text": " You're back!"}, {"control": {"kind": "pause", "length": "long"}}, {"text": "\nI mean... You've come back, have you?"}]}, "EventFlowMsg/ArmorProcessingEventTag:Talk04": {"attributes": "GreatFairyBeforeRevival", "contents": [{"control": {"kind": "sound2", "unknown": [10, 205]}}, {"text": "What's this?"}, {"control": {"kind": "pause", "length": "long"}}, {"text": " You don't have enough\nrupees?\n\nHow tragic..."}, {"control": {"kind": "pause", "length": "long"}}, {"text": " How dreadfully tragic."}]}, "EventFlowMsg/ArmorProcessingEventTag:Talk06": {"attributes": "GreatFairyBeforeRevival", "contents": [{"text": "This place was once a beautiful spring...\n\n\nBut as time passed, fewer and fewer\ntravelers arrived to offer me rupees."}, {"control": {"kind": "pause", "length": "short"}}, {"text": " As a\nresult, my power has abandoned me.\nI'm nearly powerless now, so I beg your\nhelp. I need rupees to become whole\nagain."}]}, "EventFlowMsg/ArmorProcessingEventTag:Talk07": {"attributes": "GreatFairyBeforeRevival", "contents": [{"text": "Hast thou..."}, {"control": {"kind": "pause", "length": "short"}}, {"text": " I mean..."}, {"control": {"kind": "pause", "length": "short"}}, {"text": " Have you ever felt\nthe pain of losing a horse companion?\n\nIf my power is returned to me,"}, {"control": {"kind": "pause", "length": "short"}}, {"text": " I may be\nable to help you out in some way...\n\nSo please... I beg of you..."}, {"control": {"kind": "pause", "length": "longer"}}, {"text": "\nHelp me regain the power I've lost..."}]}, "EventFlowMsg/ArmorProcessingEventTag:Talk08": {"attributes": "GreatFairyBeforeRevival", "contents": [{"control": {"kind": "sound2", "unknown": [9, 205]}}, {"text": "Thank you. You are too kind."}]}, "EventFlowMsg/ArmorProcessingEventTag:Talk09": {"attributes": "GreatFairyBeforeRevival", "contents": [{"text": "Aah... "}, {"control": {"kind": "pause", "length": "short"}}, {"text": "The power..."}]}, "EventFlowMsg/ArmorProcessingEventTag:Talk09_1": {"attributes": "GreatFairyBeforeRevival", "contents": [{"text": "Nwaaaaaagh... "}, {"control": {"kind": "pause", "length": "short"}}, {"text": "I..."}, {"control": {"kind": "pause", "length": "short"}}, {"text": "am..."}]}, "EventFlowMsg/ArmorProcessingEventTa