{"EventFlowMsg/Npc_HighMountain003:0000": {"attributes": "", "contents": [{"text": "Uh... Yeah."}]}, "EventFlowMsg/Npc_HighMountain003:0001": {"attributes": "", "contents": [{"text": "Nope."}]}, "EventFlowMsg/Npc_HighMountain003:0002": {"attributes": "", "contents": [{"text": "I love stories!"}]}, "EventFlowMsg/Npc_HighMountain003:0003": {"attributes": "", "contents": [{"text": "No, thanks."}]}, "EventFlowMsg/Npc_HighMountain003:0004": {"attributes": "", "contents": [{"text": "Well? What was it?!"}]}, "EventFlowMsg/Npc_HighMountain003:0008": {"attributes": "", "contents": [{"text": "I found it."}]}, "EventFlowMsg/Npc_HighMountain003:0009": {"attributes": "", "contents": [{"text": "Well..."}]}, "EventFlowMsg/Npc_HighMountain003:near00": {"attributes": "Npc_HighMountain003", "contents": [{"text": "\u266a"}]}, "EventFlowMsg/Npc_HighMountain003:near01": {"attributes": "Npc_HighMountain003", "contents": [{"text": "*yawn*"}]}, "EventFlowMsg/Npc_HighMountain003:near02": {"attributes": "Npc_HighMountain003", "contents": [{"text": "A birdie!"}]}, "EventFlowMsg/Npc_HighMountain003:talk02": {"attributes": "Npc_HighMountain003", "contents": [{"control": {"kind": "sound", "unknown": [0, 6]}}, {"text": "I'm busy right now...\n\n\nBut if you follow me, I'll tell you a story."}]}, "EventFlowMsg/Npc_HighMountain003:talk04": {"attributes": "Npc_HighMountain003", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Let's talk later..."}]}, "EventFlowMsg/Npc_HighMountain003:talk05": {"attributes": "Npc_HighMountain003", "contents": [{"control": {"kind": "sound", "unknown": [9, 0]}}, {"text": "I'm gonna go sleepy now..."}]}, "EventFlowMsg/Npc_HighMountain003:talk06": {"attributes": "Npc_HighMountain003", "contents": [{"control": {"kind": "sound", "unknown": [11, 34]}}, {"text": "...Aren't you going to say\n\"good morning\" to me?"}, {"control": {"kind": "choice", "choice_labels": [0, 1], "selected_index": 0, "cancel_index": 2, "unknown": 6}}]}, "EventFlowMsg/Npc_Hig