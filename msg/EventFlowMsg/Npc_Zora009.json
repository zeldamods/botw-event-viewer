{"EventFlowMsg/Npc_Zora009:0003": {"attributes": "", "contents": [{"text": "Good-bye."}]}, "EventFlowMsg/Npc_Zora009:0010": {"attributes": "", "contents": [{"text": "Marot Mart?"}]}, "EventFlowMsg/Npc_Zora009:0011": {"attributes": "", "contents": [{"text": "The general store?"}]}, "EventFlowMsg/Npc_Zora009:0012": {"attributes": "", "contents": [{"text": "The workshop?"}]}, "EventFlowMsg/Npc_Zora009:near00": {"attributes": "Npc_Zora009", "contents": [{"control": {"kind": "sound2", "unknown": [1, 205]}}, {"text": "Welcome!"}]}, "EventFlowMsg/Npc_Zora009:talk00": {"attributes": "Npc_Zora009", "contents": [{"control": {"kind": "sound", "unknown": [1, 2]}}, {"text": "Good morning!"}]}, "EventFlowMsg/Npc_Zora009:talk01": {"attributes": "Npc_Zora009", "contents": [{"control": {"kind": "sound", "unknown": [1, 2]}}, {"text": "Good day!"}]}, "EventFlowMsg/Npc_Zora009:talk02": {"attributes": "Npc_Zora009", "contents": [{"control": {"kind": "sound", "unknown": [1, 2]}}, {"text": "Good evening!"}]}, "EventFlowMsg/Npc_Zora009:talk03": {"attributes": "Npc_Zora009", "contents": [{"control": {"kind": "sound", "unknown": [1, 1]}}, {"text": "\u266aWelcome to Marot Mart!\u266a"}]}, "EventFlowMsg/Npc_Zora009:talk04": {"attributes": "Npc_Zora009", "contents": [{"control": {"kind": "sound", "unknown": [1, 1]}}, {"text": "This is where you'll find our general\nstore, the Coral Reef. "}, {"control": {"kind": "pause", "length": "longer"}}, {"text": "Our workshop,\nthe Hammerhead, is also here."}]}, "EventFlowMsg/Npc_Zora009:talk05": {"attributes": "Npc_Zora009", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Yes, yes!"}]}, "EventFlowMsg/Npc_Zora009:talk06": {"attributes": "Npc_Zora009", "contents": [{"control": {"kind": "sound", "unknown": [7, 1]}}, {"text": "Do you need me for something?"}, {"control": {"kind": "raw", "one": {"eight": {"unknown_1": [], "field_1": [10, 0, 11, 0, 12, 0, 3], "field_2": [0, 0, 0, 3]}}}}]}, "EventFlowMsg/Npc_Zora009:talk07": {"attributes": "Npc_Zora009", "contents": [{"control": {"kind": "sound", "unknown": [1, 10]}}, {"text": "See ya later!"}]}, "EventFlowMsg/Npc_Zora009:talk08": {"attributes": "Npc_Zora009", "contents": [{"control": {"kind": "sound", "unknown": [7, 9]}}, {"text": "\u266aYep!\u266a\n\n\nThe general store and the workshop...\nThey're the best shops EVER!"}, {"control": {"kind": "pause", "length": "longer"}}, {"text": "\nAnd together they're caaaaalled..."}]}, "EventFlowMsg/Npc_Zora009:talk09": {"attributes": "Npc_Zora009", "contents": [{"control": {"kind": "sound", "unknown": [1, 1]}}, {"control": {"kind": "animation", "name": "Humming_00"}}, {"text": "\u266aMarot Mart!\u266a\n\n\n"}, {"control": {"kind": "sound", "unknown": [1, 1]}}, {"control": {"kind": "animation", "name": "Humming_01"}}, {"text": "\u266aMarot Maaaaaart!"}, {"control": {"kind": "pause", "length": "long"}}, {"text": "\nLa de da de daaa!\u266a\n\n"}, {"control": {"kind": "text_size", "percent": 125}}, {"control": {"kind": "sound", "unknown": [7, 11]}}, {"text": "\u266aLA DEE DAAAAA!\u266a"}, {"control": {"kind": "text_size", "percent": 100}}]}, "EventFlowMsg/Npc_Zora009:talk10": {"attributes": "Npc_Zora009", "contents": [{"control": {"kind": "sound", "unknown": [7, 1]}}, {"text": "That's my theme song!"}, {"control": {"kind": "pause", "length": "short"}}, {"text": " Isn't it cute?!"}]}, "EventFlowMsg/Npc_Zora009:talk11": {"attributes": "Npc_Zora009", "contents": [{"control": {"kind": "sound", "unknown": [0, 9]}}, {"text": "The old man in the workshop, his name\nis Dento..."}, {"control": {"kind": "pause", "length": "short"}}, {"text": " He's my grandpa!\n\nHe's the one who fixed Mipha's weapon,"}, {"control": {"kind": "pause", "length": "short"}}, {"text": "\nthe "}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "Lightscale Trident"}, {"control": {"kind": "reset_colour"}}, {"text": "."}]}, "EventFlowMsg/Npc_Zora009:talk12": {"attributes": "Npc_Zora009", "contents": [{"control": {"kind": "sound", "unknown": [1, 4]}}, {"text": "Our general store has been in business\nfor over 100 years."}, {"control": {"kind": "pause", "length": "short"}}, {"text": " It has a long and\nhonorable history.\nThe shopkeep, Cleff, is really serious."}, {"control": {"kind": "pause", "length": "short"}}, {"text": "\nBut his younger sister, Tona, is head\nover fins for Prince Sidon! Kee hee!"}]}, "EventFlowMsg/Npc_Zora009:talk13": {"attributes": "Npc_Zora009", "contents": [{"control": {"kind": "sound", "unknown": [1, 6]}}, {"text": "Is there anything else you'd like to\nask me?"}, {"control": {"kind": "raw", "one": {"eight": {"unknown_1": [], "field_1": [10, 0, 11, 0, 12, 0, 3], "field_2": [0, 0, 0, 3]}}}}]}, "EventFlowMsg/Npc_Zora009:talk14": {"attributes": "Npc_Zora009", "contents": [{"control": {"kind": "sound", "unknown": [7, 4]}}, {"text": "You want me to sing again?! "}, {"control": {"kind": "pause", "length": "long"}}, {"text": "Kee hee..."}, {"control": {"kind": "pause", "length": "short"}}, {"text": "\n\u266aYou got it!\u266a"}]}, "EventFlowMsg/Npc_Zora009:talk15": {"attributes": "Npc_Zora009", "contents": [{"control": {"kind": "sound", "unknown": [7, 4]}}, {"text": "Or...do you need something else?"}, {"control": {"kind": "raw", "one": {"eight": {"unknown_1": [], "field_1": [10, 0, 11, 0, 12, 0, 3], "field_2": [0, 0, 0, 3]}}}}]}}