{"EventFlowMsg/Npc_TamourHatago2_001:0000": {"attributes": "", "contents": [{"text": "I'm a humble traveler."}]}, "EventFlowMsg/Npc_TamourHatago2_001:0001": {"attributes": "", "contents": [{"text": "..."}]}, "EventFlowMsg/Npc_TamourHatago2_001:0002": {"attributes": "", "contents": [{"text": "I'm the best!"}]}, "EventFlowMsg/Npc_TamourHatago2_001:0003": {"attributes": "", "contents": [{"text": "No, not at all."}]}, "EventFlowMsg/Npc_TamourHatago2_001:0004": {"attributes": "", "contents": [{"text": "Legendary sword?"}]}, "EventFlowMsg/Npc_TamourHatago2_001:0005": {"attributes": "", "contents": [{"text": "I'm not interested."}]}, "EventFlowMsg/Npc_TamourHatago2_001:0006": {"attributes": "", "contents": [{"text": "Yes."}]}, "EventFlowMsg/Npc_TamourHatago2_001:0007": {"attributes": "", "contents": [{"text": "No."}]}, "EventFlowMsg/Npc_TamourHatago2_001:0008": {"attributes": "", "contents": [{"text": "It's legendary."}]}, "EventFlowMsg/Npc_TamourHatago2_001:0009": {"attributes": "", "contents": [{"text": "It's a fake."}]}, "EventFlowMsg/Npc_TamourHatago2_001:talk00": {"attributes": "Npc_TamourHatago2_001", "contents": [{"control": {"kind": "sound", "unknown": [6, 2]}}, {"text": "Heyo. Who are you?!"}, {"control": {"kind": "choice", "choice_labels": [0, 1], "selected_index": 0, "cancel_index": 2, "unknown": 6}}]}, "EventFlowMsg/Npc_TamourHatago2_001:talk01": {"attributes": "Npc_TamourHatago2_001", "contents": [{"control": {"kind": "sound", "unknown": [2, 14]}}, {"text": "*tsk*"}, {"control": {"kind": "pause", "length": "short"}}, {"text": " Ignoring me, I see."}]}, "EventFlowMsg/Npc_TamourHatago2_001:talk02": {"attributes": "Npc_TamourHatago2_001", "contents": [{"control": {"kind": "sound", "unknown": [0, 1]}}, {"text": "Hmph. "}, {"control": {"kind": "pause", "length": "short"}}, {"text": "Show-off."}]}, "EventFlowMsg/Npc_TamourHatago2_001:talk04": {"attributes": "Npc_TamourHatago2_001", "contents": [{"control": {"kind": "sound", "unknown": [11, 0]}}, {"text": "Traveling 