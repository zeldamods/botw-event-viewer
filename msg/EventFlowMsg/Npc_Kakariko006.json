{"EventFlowMsg/Npc_Kakariko006:0000": {"attributes": "", "contents": [{"text": "I'll take it."}]}, "EventFlowMsg/Npc_Kakariko006:0001": {"attributes": "", "contents": [{"text": "Bye."}]}, "EventFlowMsg/Npc_Kakariko006:0002": {"attributes": "", "contents": [{"text": "Tell me your secrets!"}]}, "EventFlowMsg/Npc_Kakariko006:0003": {"attributes": "", "contents": [{"text": "Not interested."}]}, "EventFlowMsg/Npc_Kakariko006:0004": {"attributes": "", "contents": [{"text": "Secret art?"}]}, "EventFlowMsg/Npc_Kakariko006:0005": {"attributes": "", "contents": [{"text": "Dodge jump."}]}, "EventFlowMsg/Npc_Kakariko006:0006": {"attributes": "", "contents": [{"text": "Perfect block."}]}, "EventFlowMsg/Npc_Kakariko006:0007": {"attributes": "", "contents": [{"text": "I saw that..."}]}, "EventFlowMsg/Npc_Kakariko006:0008": {"attributes": "", "contents": [{"text": "I saw nothing..."}]}, "EventFlowMsg/Npc_Kakariko006:0009": {"attributes": "", "contents": [{"text": "Side jump."}]}, "EventFlowMsg/Npc_Kakariko006:0010": {"attributes": "", "contents": [{"text": "Backflip."}]}, "EventFlowMsg/Npc_Kakariko006:0013": {"attributes": "", "contents": [{"text": "Cookin'?"}]}, "EventFlowMsg/Npc_Kakariko006:Near00": {"attributes": "Npc_Kakariko006", "contents": [{"text": "Want some pumpkins?"}]}, "EventFlowMsg/Npc_Kakariko006:Near01": {"attributes": "Npc_Kakariko006", "contents": [{"text": "Rain won't stop ME!"}]}, "EventFlowMsg/Npc_Kakariko006:Near02": {"attributes": "Npc_Kakariko006", "contents": [{"text": "Aha ha!"}]}, "EventFlowMsg/Npc_Kakariko006:Near03": {"attributes": "Npc_Kakariko006", "contents": [{"text": "Hyah! Hyah!"}]}, "EventFlowMsg/Npc_Kakariko006:Near04": {"attributes": "Npc_Kakariko006", "contents": [{"text": "I need to grow more..."}]}, "EventFlowMsg/Npc_Kakariko006:Near05": {"attributes": "Npc_Kakariko006", "contents": [{"text": "Another beautiful day..."}]}, "EventFlowMsg/Npc_Kakariko006:Near06": {"attributes": "Npc_Kakariko006", "contents": [{"text": "Ah, the rain of life!"}]}, "EventFlowMsg/Npc_Kakariko006:Near07": {"attributes": "Npc_Kakariko006", "contents": [{"text": "So hungry..."}]}, "EventFlowMsg/Npc_Kakariko006:Near08": {"attributes": "Npc_Kakariko006", "contents": [{"text": "My stress just melts away!"}]}, "EventFlowMsg/Npc_Kakariko006:Near09": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [11, 0]}}, {"text": "Pumpkins are superior..."}]}, "EventFlowMsg/Npc_Kakariko006:Near10": {"attributes": "Npc_Kakariko006", "contents": [{"text": "I should head home."}]}, "EventFlowMsg/Npc_Kakariko006:Near11": {"attributes": "Npc_Kakariko006", "contents": [{"text": "I'm exhausted today."}]}, "EventFlowMsg/Npc_Kakariko006:SupriseHouse": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [10, 1]}}, {"control": {"kind": "sound2", "unknown": [19, 205]}}, {"text": "Baaah! Hm?!"}]}, "EventFlowMsg/Npc_Kakariko006:SupriseHouse_00": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [9, 18]}}, {"text": "Oh! It's you.\n\n\n"}, {"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "No one's usually home, other than\nmy wife. So, yeah..."}, {"control": {"kind": "pause", "length": "long"}}, {"text": " Don't scare me\nlike that!"}]}, "EventFlowMsg/Npc_Kakariko006:SupriseHouse_01": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [0, 1]}}, {"text": "..."}, {"control": {"kind": "pause", "length": "longer"}}]}, "EventFlowMsg/Npc_Kakariko006:Talk00": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [0, 2]}}, {"text": "Did you enjoy that "}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "fortified pumpkin"}, {"control": {"kind": "reset_colour"}}, {"text": "?"}]}, "EventFlowMsg/Npc_Kakariko006:Talk01": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [1, 9]}}, {"text": "Ha! You've got a keen eye to pick\nthat one."}]}, "EventFlowMsg/Npc_Kakariko006:Talk02": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [1, 9]}}, {"text": "I'm so glad my beloved pumpkins are\nuseful to somebody.\n\nI'll work extra hard to grow an even\nbetter batch next time!"}]}, "EventFlowMsg/Npc_Kakariko006:Talk03": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [9, 13]}}, {"text": "Mmm..."}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"control": {"kind": "pause", "length": "short"}}, {"text": " "}, {"control": {"kind": "reset_colour"}}, {"text": "It's really delicious, though..."}]}, "EventFlowMsg/Npc_Kakariko006:Talk04": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [0, 9]}}, {"text": "Ha!"}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": " "}, {"control": {"kind": "reset_colour"}}, {"text": "You wanna buy a "}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "fortified pumpkin"}, {"control": {"kind": "reset_colour"}}, {"text": ",\ndon'tcha? "}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"control": {"kind": "pause", "length": "short"}}, {"control": {"kind": "reset_colour"}}, {"text": "Of course ya do!"}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"control": {"kind": "pause", "length": "short"}}, {"control": {"kind": "reset_colour"}}, {"text": " As it were,\nI'm heading to the fields right now...\nMind waiting just a bit longer?"}]}, "EventFlowMsg/Npc_Kakariko006:Talk05": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [11, 25]}}, {"text": "Sorry, mister. It's dinnertime now."}, {"control": {"kind": "pause", "length": "longer"}}, {"text": "\nIf this is about my pumpkins,\ncan it wait until tomorrow?"}]}, "EventFlowMsg/Npc_Kakariko006:Talk06": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "set_colour", "colour": "blue"}}, {"control": {"kind": "sound", "unknown": [0, 9]}}, {"control": {"kind": "reset_colour"}}, {"text": "That'll cost ya "}, {"control": {"kind": "set_colour", "colour": "red"}}, {"control": {"kind": "variable", "variable_kind": 16, "name": "PlacedItemPrice"}}, {"text": " rupees"}, {"control": {"kind": "reset_colour"}}, {"control": {"kind": "pause", "length": "longer"}}, {"text": ".\nInterested?"}, {"control": {"kind": "choice", "choice_labels": [0, 1], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_Kakariko006:Talk07": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [7, 1]}}, {"control": {"kind": "sound2", "unknown": [21, 205]}}, {"text": "My wife's "}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "fortified pumpkin"}, {"control": {"kind": "reset_colour"}}, {"text": " dishes\nare the best.\n\n"}, {"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Way better than a barrel o' carrots,\nthat's for sure!"}]}, "EventFlowMsg/Npc_Kakariko006:Talk08": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [11, 0]}}, {"text": "Sorry to interrupt, but..."}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"control": {"kind": "pause", "length": "short"}}, {"control": {"kind": "reset_colour"}}, {"text": "\n\n\nThis plum garden is very special to \nmy wife."}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"control": {"kind": "pause", "length": "short"}}, {"text": " "}, {"control": {"kind": "reset_colour"}}, {"text": "Please be considerate\nof that."}]}, "EventFlowMsg/Npc_Kakariko006:Talk09": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [10, 1]}}, {"control": {"kind": "sound2", "unknown": [17, 205]}}, {"text": "Who's there?"}]}, "EventFlowMsg/Npc_Kakariko006:Talk10": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "What are you doing out in the middle\nof the night?"}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"control": {"kind": "pause", "length": "short"}}, {"control": {"kind": "reset_colour"}}, {"text": "\n\nAs for me, the first step to achieving\ngreat defense is training your legs.\nThat's why I'm running at this hour.\nTraining is the best way to learn the\nsecret art of defense, after all."}, {"control": {"kind": "choice", "choice_labels": [4, 1], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_Kakariko006:Talk11": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [0, 9]}}, {"text": "The Sheikah tribe is exceedingly wise."}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"control": {"kind": "pause", "length": "short"}}, {"control": {"kind": "reset_colour"}}, {"text": "\nThat is because you cannot protect\nwhat you love with force alone.\nI know everyone has their own way of\nfighting, but please try to remember that."}, {"control": {"kind": "choice", "choice_labels": [4, 5, 6, 3], "selected_index": 0, "cancel_index": 3, "unknown": 10}}]}, "EventFlowMsg/Npc_Kakariko006:Talk12": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Long ago, a wise master known simply\nas the swordsman was enshrined\u2014\nright in this village, no less.\nIt is said that if you overcome his trial,\nyou will master the secret art of defense.\n\n"}, {"control": {"kind": "sound", "unknown": [11, 0]}}, {"text": "However, no one knows exactly what\nthis \"trial\" entails...\n\n"}, {"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "That is why I must stay vigilant with my\ntraining, so that I am ready whenever\nthis trial presents itself to me!"}]}, "EventFlowMsg/Npc_Kakariko006:Talk13": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "There are savage old men out there"}, {"control": {"kind": "pause", "length": "short"}}, {"text": " who\ndo nothing but prattle on about how the\nbest defense is a good offense...\nBut I know better. I believe that one\nwho masters the art of defense is the\nstrongest of all."}]}, "EventFlowMsg/Npc_Kakariko006:Talk14": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Keep focusing on the enemy, and jump to\nthe side."}, {"control": {"kind": "pause", "length": "long"}}, {"text": " We call this a side jump.\n\nHeh, see that? You get to look stylish \neven while minding your defenses!"}]}, "EventFlowMsg/Npc_Kakariko006:Talk16": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [1, 23]}}, {"text": "Ha! It's rare to see a young lad\nlike you in a village like this!"}]}, "EventFlowMsg/Npc_Kakariko006:Talk18": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "set_colour", "colour": "blue"}}, {"control": {"kind": "sound", "unknown": [0, 11]}}, {"text": "Fortified pumpkins "}, {"control": {"kind": "reset_colour"}}, {"text": "make you stronger.\nI ask the old lady to keep 'em cookin'"}, {"control": {"kind": "raw", "four": {"three": {"field_1": 0}}}}, {"text": "\nso I can eat some every day."}, {"control": {"kind": "choice", "choice_labels": [13, 1], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_Kakariko006:Talk19": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [1, 9]}}, {"text": "Ha! I'm just pullin' your leg!"}, {"control": {"kind": "pause", "length": "long"}}, {"text": " You looked\nso serious,"}, {"control": {"kind": "pause", "length": "longer"}}, {"text": " I couldn't help but goad\nya a bit."}]}, "EventFlowMsg/Npc_Kakariko006:Talk20": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [10, 1]}}, {"control": {"kind": "sound2", "unknown": [20, 205]}}, {"text": "OH! "}, {"control": {"kind": "pause", "length": "short"}}, {"text": "Oh... It's you."}, {"control": {"kind": "pause", "length": "short"}}, {"text": "\nWh-what do you need?"}, {"control": {"kind": "choice", "choice_labels": [7, 8], "selected_index": 0, "cancel_index": 2, "unknown": 6}}]}, "EventFlowMsg/Npc_Kakariko006:Talk21": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "set_colour", "colour": "blue"}}, {"control": {"kind": "sound", "unknown": [1, 9]}}, {"text": "Fortified pumpkins "}, {"control": {"kind": "reset_colour"}}, {"text": "are Kakariko Village's\nspecialty."}, {"control": {"kind": "pause", "length": "long"}}, {"text": " I put my heart and soul into\nevery one of 'em. They're delicious.\nIf you want more, I can give you some."}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"control": {"kind": "pause", "length": "short"}}, {"control": {"kind": "reset_colour"}}, {"text": "\nJust stop by the fields and say hello."}]}, "EventFlowMsg/Npc_Kakariko006:Talk22": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "It's getting late. I should head home."}]}, "EventFlowMsg/Npc_Kakariko006:Talk23": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [1, 9]}}, {"text": "Ha! That old geezer is as savage\nas ever.\n\nHe's always running around yelling\nnonsense like, \"The best defense is\na good offense!\""}]}, "EventFlowMsg/Npc_Kakariko006:Talk24": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [0, 32]}}, {"text": "When I was young, I used to dash\nthrough the village like the wind.\n\nPeople knew me as the Olkin who never\nran out of breath, never slowed down.\n\n"}, {"control": {"kind": "sound", "unknown": [9, 1]}}, {"text": "Unfortunately, you can't outrun aging...\nno matter how hard you train.\nTrust me, I've tried."}]}, "EventFlowMsg/Npc_Kakariko006:Talk25": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "If there are any other skills you'd like\nto learn, just let me know."}, {"control": {"kind": "choice", "choice_labels": [4, 5, 6, 3], "selected_index": 0, "cancel_index": 3, "unknown": 10}}]}, "EventFlowMsg/Npc_Kakariko006:Talk26": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "What are you doing out on\nsuch a rainy night?"}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"control": {"kind": "pause", "length": "short"}}, {"control": {"kind": "reset_colour"}}, {"text": "\n\nAs for me, the first step to achieving\ngreat defense is training your legs.\nThat's why I'm running at this hour.\nTraining is the best way to learn the\nsecret art of defense, after all."}, {"control": {"kind": "choice", "choice_labels": [4, 1], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_Kakariko006:Talk27": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [9, 13]}}, {"text": "It seems..."}, {"control": {"kind": "pause", "length": "long"}}, {"text": "you caught me.\n\n\nI could never get along with Steen..."}, {"control": {"kind": "pause", "length": "long"}}, {"text": "\nno matter how I tried.\n\nEven when we'd go hunting together,\nI'd slowly crouch down to get close to\nour prey, but he'd just charge in yelling.\n"}, {"control": {"kind": "sound", "unknown": [8, 7]}}, {"text": "\"Offense is kiiing!\""}, {"control": {"kind": "pause", "length": "long"}}, {"text": "\nThat's what he'd bellow.\nAnnoyed the tar outta me.\n"}, {"control": {"kind": "sound", "unknown": [9, 1]}}, {"text": "Needless to say, our prey always ran...\nOn top of that, he just refuses to sell\nmy pumpkins in his "}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "shop"}, {"control": {"kind": "reset_colour"}}, {"text": "!\n"}, {"control": {"kind": "sound2", "unknown": [10, 205]}}, {"text": "But... My, what have I done?"}, {"control": {"kind": "pause", "length": "long"}}, {"text": "\nI'm just so mortified..."}]}, "EventFlowMsg/Npc_Kakariko006:Talk28": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [1, 18]}}, {"text": "Ha! Then I suppose all is well.\n\n\nHowever... I'm a tad busy right now.\n\n\nIf ya need something, mind coming\nback later?"}]}, "EventFlowMsg/Npc_Kakariko006:Talk29": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [8, 22]}}, {"text": "You aren't..."}, {"control": {"kind": "pause", "length": "long"}}, {"text": "part of the "}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "Yiga Clan"}, {"control": {"kind": "reset_colour"}}, {"text": ",\nare you?\n\n..."}, {"control": {"kind": "pause", "length": "longer"}}, {"text": "\n"}]}, "EventFlowMsg/Npc_Kakariko006:Talk30": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [0, 24]}}, {"text": "But being caught in the act "}, {"control": {"kind": "pause", "length": "longer"}}, {"text": "makes\nme realize what I've become...\n\nAs of today...I'll stop taking my anger\nout on the crops."}]}, "EventFlowMsg/Npc_Kakariko006:Talk31": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Jumping is one of the basics of dodging."}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"control": {"kind": "pause", "length": "short"}}, {"control": {"kind": "reset_colour"}}, {"text": "\nYou'll want to at least master these\ntwo skills."}, {"control": {"kind": "choice", "choice_labels": [9, 10], "selected_index": 0, "cancel_index": 2, "unknown": 6}}]}, "EventFlowMsg/Npc_Kakariko006:Talk32": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Keep focusing on your foe, and then\njump back."}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"control": {"kind": "pause", "length": "short"}}, {"text": " "}, {"control": {"kind": "reset_colour"}}, {"text": "We call this a backflip.\n\n"}, {"control": {"kind": "sound2", "unknown": [7, 205]}}, {"text": "Ha! See that? Even an old man like me\nknows what's hip and cool!"}]}, "EventFlowMsg/Npc_Kakariko006:Talk34": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "We call those who swear allegiance to\n"}, {"control": {"kind": "set_colour", "colour": "red"}}, {"text": "Calamity Ganon "}, {"control": {"kind": "reset_colour"}}, {"text": "the "}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "Yiga Clan"}, {"control": {"kind": "reset_colour"}}, {"text": ". They...\n\n"}, {"control": {"kind": "sound", "unknown": [11, 1]}}, {"control": {"kind": "sound2", "unknown": [15, 205]}}, {"text": "Well, let's just say they don't take kindly\nto those who would stand against "}, {"control": {"kind": "set_colour", "colour": "red"}}, {"text": "Ganon"}, {"control": {"kind": "reset_colour"}}, {"text": "."}, {"control": {"kind": "pause", "length": "longer"}}, {"text": "\n\nIt seems they're active again, "}, {"control": {"kind": "pause", "length": "longer"}}, {"text": "ever since\nthat big earthquake the other day.\n\n"}, {"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "But don't worry. "}, {"control": {"kind": "pause", "length": "short"}}, {"text": "I don't think you're part\nof that awful "}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "Yiga Clan"}, {"control": {"kind": "reset_colour"}}, {"text": ". "}, {"control": {"kind": "pause", "length": "longer"}}, {"text": "I can figure that\nmuch out just by lookin' at ya."}]}, "EventFlowMsg/Npc_Kakariko006:Talk35": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Use a "}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "fortified pumpkin "}, {"control": {"kind": "reset_colour"}}, {"text": "in your cooking\nto make a dish as firm as my abs.\nFood like that will increase your defense.\nPutting lots of "}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "fortified pumpkin "}, {"control": {"kind": "reset_colour"}}, {"text": "in the\ndish is key. Go on\u2014experiment a bit!"}]}, "EventFlowMsg/Npc_Kakariko006:Talk36": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [11, 0]}}, {"text": "There's a hotheaded geezer who says,\n\"The best defense is a good offense.\" \nPoppycock, I say!\n"}, {"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "I firmly believe the best offense\nis a good defense."}]}, "EventFlowMsg/Npc_Kakariko006:Talk37": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "text_size", "percent": 80}}, {"control": {"kind": "sound2", "unknown": [3, 205]}}, {"text": "I'll protect you...my sweet wife..."}, {"control": {"kind": "text_size", "percent": 100}}]}, "EventFlowMsg/Npc_Kakariko006:Talk39": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [0, 9]}}, {"text": "Well, whaddaya know."}]}, "EventFlowMsg/Npc_Kakariko006:Talk41": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [11, 0]}}, {"text": "Sorry to bother you when you're having\na good time, but... "}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"control": {"kind": "pause", "length": "short"}}, {"control": {"kind": "reset_colour"}}, {"text": "Don't you think\nyou're going a bit overboard?\nThis plum garden is very special to\nmy wife."}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"control": {"kind": "pause", "length": "short"}}, {"text": " "}, {"control": {"kind": "reset_colour"}}, {"text": "Please be considerate\nof that."}]}, "EventFlowMsg/Npc_Kakariko006:Talk42": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [10, 1]}}, {"control": {"kind": "sound2", "unknown": [20, 205]}}, {"text": "WHAT ARE YOU DOING?!"}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"control": {"kind": "pause", "length": "longer"}}, {"text": " "}, {"control": {"kind": "reset_colour"}}, {"text": "What happened\nto the plum trees?!\n\n"}, {"control": {"kind": "sound", "unknown": [9, 1]}}, {"text": "This plum garden is very special to my\nwife."}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"control": {"kind": "pause", "length": "short"}}, {"text": " "}, {"control": {"kind": "reset_colour"}}, {"text": "She's going to be awfully sad...\n\n"}, {"control": {"kind": "sound", "unknown": [0, 1]}}, {"control": {"kind": "sound2", "unknown": [10, 205]}}, {"text": "They might just be plums to you, but to\nsome people, they're little treasures.\n\nDon't you forget that!"}]}, "EventFlowMsg/Npc_Kakariko006:Talk43": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Oof, I worked my tail off today!\nNow time for some grub!"}]}, "EventFlowMsg/Npc_Kakariko006:Talk44": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [1, 1]}}, {"control": {"kind": "sound2", "unknown": [21, 205]}}, {"text": "My wife's tough dishes"}, {"control": {"kind": "set_colour", "colour": "red"}}, {"text": " "}, {"control": {"kind": "reset_colour"}}, {"text": "are made from\n"}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "fortified pumpkins"}, {"control": {"kind": "reset_colour"}}, {"text": ". They're out of\nthis world.\nThere are A LOT of "}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "fortified pumpkins"}, {"control": {"kind": "reset_colour"}}, {"text": "\nin that recipe, I tell ya what."}]}, "EventFlowMsg/Npc_Kakariko006:Talk45": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "It's raining now... I suppose I should\nhead home."}]}, "EventFlowMsg/Npc_Kakariko006:Talk46": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Phew! I worked my tail off today.\nI should warm up with some grub\nbefore I catch a cold in this rain."}]}, "EventFlowMsg/Npc_Kakariko006:Talk47": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "I'm feeling worn out all of a sudden...\nProbably time to turn in."}]}, "EventFlowMsg/Npc_Kakariko006:Talk48": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [0, 19]}}, {"text": "Hm?"}]}, "EventFlowMsg/Npc_Kakariko006:Talk49": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [10, 1]}}, {"control": {"kind": "sound2", "unknown": [20, 205]}}, {"text": "OH! "}, {"control": {"kind": "pause", "length": "short"}}, {"text": "Oh... "}, {"control": {"kind": "pause", "length": "short"}}, {"text": "You're still here, eh?"}, {"control": {"kind": "choice", "choice_labels": [7, 8], "selected_index": 0, "cancel_index": 2, "unknown": 6}}]}, "EventFlowMsg/Npc_Kakariko006:Talk50": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [9, 13]}}, {"text": "You caught me again..."}, {"control": {"kind": "pause", "length": "longer"}}, {"text": "\n\n\nI try to stop myself, but this grudge has\ngrown over the years. "}, {"control": {"kind": "pause", "length": "longer"}}, {"text": "It takes me over.\n\n"}, {"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "No! The past is behind me! It really is...\n\n\n"}, {"control": {"kind": "sound", "unknown": [9, 0]}}, {"text": "Still..."}, {"control": {"kind": "pause", "length": "short"}}, {"text": "I just can't stand the fact\nthat he refuses to stock my\npumpkins at his "}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "shop"}, {"control": {"kind": "reset_colour"}}, {"text": "!"}]}, "EventFlowMsg/Npc_Kakariko006:Talk51": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [9, 1]}}, {"text": "But whatever the reason may be, I plain\nshouldn't be doing such things...\n\n"}, {"control": {"kind": "sound", "unknown": [0, 24]}}, {"text": "I've come to my senses now that you've\ncaught me again."}, {"control": {"kind": "pause", "length": "longer"}}, {"text": " Thank you.\n\nAs of today...I'll stop taking my anger\nout on the crops."}]}, "EventFlowMsg/Npc_Kakariko006:Talk52": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [4, 1]}}, {"control": {"kind": "sound2", "unknown": [19, 205]}}, {"text": "You... "}, {"control": {"kind": "pause", "length": "longer"}}, {"text": "You're still here?\n\n\n"}, {"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "A lot happened today, but it's the perfect\nnight to train and try to get my mind off\nthe foolish things that distract it.\nTraining is the best way to learn the\nsecret art of defense, after all."}, {"control": {"kind": "choice", "choice_labels": [4, 1], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_Kakariko006:Talk53": {"attributes": "NPC_GodVoice", "contents": [{"control": {"kind": "raw", "one": {"two": {"field_1": 0}}}}, {"text": "That's a mighty fine pumpkin...\nYou'll get in big trouble if you\ntake it without permission.\nCome back when the owner is here."}]}, "EventFlowMsg/Npc_Kakariko006:Talk54": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [1, 11]}}, {"text": "Thank goodness!"}]}, "EventFlowMsg/Npc_Kakariko006:Talk55": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [1, 0]}}, {"text": "Please don't hold it against me."}, {"control": {"kind": "pause", "length": "short"}}, {"text": "\nHere, take this in exchange."}]}, "EventFlowMsg/Npc_Kakariko006:Talk56": {"attributes": "Npc_Kakariko006", "contents": [{"control": {"kind": "sound", "unknown": [0, 1]}}, {"text": "..."}, {"control": {"kind": "pause", "length": "longer"}}]}, "EventFlowMsg/Npc_Kakariko006:TerrorArrow": {"attributes": "", "contents": [{"text": "Respect your elders!"}]}, "EventFlowMsg/Npc_Kakariko006:TerrorBomb": {"attributes": "", "contents": [{"text": "A blue pumpkin?!"}]}, "EventFlowMsg/Npc_Kakariko006:TerrorSword": {"attributes": "", "contents": [{"text": "Save the pumpkins!"}]}}