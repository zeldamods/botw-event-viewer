{"EventFlowMsg/Npc_goron004:0000": {"attributes": "", "contents": [{"text": "I want to buy."}]}, "EventFlowMsg/Npc_goron004:0001": {"attributes": "", "contents": [{"text": "I want to sell."}]}, "EventFlowMsg/Npc_goron004:0002": {"attributes": "", "contents": [{"text": "What is this place?"}]}, "EventFlowMsg/Npc_goron004:0003": {"attributes": "", "contents": [{"text": "Good-bye."}]}, "EventFlowMsg/Npc_goron004:5500": {"attributes": "", "contents": [{"text": "OK."}]}, "EventFlowMsg/Npc_goron004:5501": {"attributes": "", "contents": [{"text": "Never mind."}]}, "EventFlowMsg/Npc_goron004:5503": {"attributes": "", "contents": [{"text": "Never mind."}]}, "EventFlowMsg/Npc_goron004:5508": {"attributes": "", "contents": [{"text": "All of them."}]}, "EventFlowMsg/Npc_goron004:Com_Talk_00": {"attributes": "", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "What do you wanna sell?"}]}, "EventFlowMsg/Npc_goron004:Com_Talk_01": {"attributes": "", "contents": [{"text": "Does "}, {"control": {"kind": "set_colour", "colour": "red"}}, {"control": {"kind": "variable", "variable_kind": 18, "name": "ShopTradePrice"}}, {"control": {"kind": "reset_colour"}}, {"text": " "}, {"control": {"kind": "set_colour", "colour": "red"}}, {"text": "rupees"}, {"control": {"kind": "reset_colour"}}, {"text": " sound OK?"}, {"control": {"kind": "choice", "choice_labels": [5500, 5501], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_goron004:Com_Talk_02": {"attributes": "", "contents": [{"control": {"kind": "sound", "unknown": [7, 0]}}, {"text": "Thanks, brother!"}]}, "EventFlowMsg/Npc_goron004:Com_Talk_02b": {"attributes": "", "contents": [{"control": {"kind": "sound", "unknown": [7, 0]}}, {"text": "You done sellin'?"}]}, "EventFlowMsg/Npc_goron004:Com_Talk_02c": {"attributes": "", "contents": [{"control": {"kind": "sound", "unknown": [7, 0]}}, {"text": "Ah. "}, {"control": {"kind": "pause", "length": "short"}}, {"text": "Thank you."}]}, "EventFlowMsg/Npc_goron004:Com_Talk_02d": {"attributes": "", "contents": [{"control": {"kind": "sound", "unknown": [7, 0]}}, {"text": "Thanks, "}, {"control": {"kind": "pause", "length": "short"}}, {"text": "little guy."}]}, "EventFlowMsg/Npc_goron004:Com_Talk_04": {"attributes": "", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Ah, OK. That's too bad."}]}, "EventFlowMsg/Npc_goron004:Com_Talk_05": {"attributes": "", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "What are ya sellin'?"}]}, "EventFlowMsg/Npc_goron004:Com_Talk_05b": {"attributes": "", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Anything else you wanna sell?"}]}, "EventFlowMsg/Npc_goron004:Com_Talk_05c": {"attributes": "", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Wanna sell anything else?"}]}, "EventFlowMsg/Npc_goron004:Com_Talk_05d": {"attributes": "", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Anything else?"}]}, "EventFlowMsg/Npc_goron004:Com_Talk_13": {"attributes": "Npc_Kakariko010", "contents": [{"text": "How many?"}, {"control": {"kind": "choice", "choice_labels": [5500, 5508, 5503], "selected_index": 0, "cancel_index": 2, "unknown": 8}}]}, "EventFlowMsg/Npc_goron004:Com_Talk_14": {"attributes": "Npc_Kakariko010", "contents": [{"text": "I can offer you "}, {"control": {"kind": "set_colour", "colour": "red"}}, {"control": {"kind": "variable", "variable_kind": 18, "name": "ShopTradePrice"}}, {"text": " rupees"}, {"control": {"kind": "reset_colour"}}, {"text": ". "}, {"control": {"kind": "choice", "choice_labels": [5500, 5501], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_goron004:Com_Talk_15": {"attributes": "Npc_Kakariko010", "contents": [{"control": {"kind": "sound", "unknown": [11, 0]}}, {"text": "Sorry, but I'm not interested in those."}]}, "EventFlowMsg/Npc_goron004:Com_Talk_16": {"attributes": "Npc_Kakariko010", "contents": [{"control": {"kind": "sound", "unknown": [11, 0]}}, {"text": "...I'm not gonna buy the clothes that\nyou're wearin'. For obvious reasons."}]}, "EventFlowMsg/Npc_goron004:Com_Talk_33": {"attributes": "Npc_Kakariko010", "contents": [{"text": "Just the one, right?"}, {"control": {"kind": "pause", "length": "short"}}, {"text": " I'll give you\n"}, {"control": {"kind": "set_colour", "colour": "red"}}, {"control": {"kind": "variable", "variable_kind": 18, "name": "ShopTradePrice"}}, {"text": " rupees "}, {"control": {"kind": "reset_colour"}}, {"text": "for it!"}, {"control": {"kind": "choice", "choice_labels": [5500, 5501], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_goron004:Com_Talk_35": {"attributes": "", "contents": [{"text": "So all of 'em?"}, {"control": {"kind": "pause", "length": "longer"}}, {"text": " I'll buy those off ya for\n"}, {"control": {"kind": "set_colour", "colour": "red"}}, {"control": {"kind": "variable", "variable_kind": 18, "name": "ShopTradePrice"}}, {"text": " rupees"}, {"control": {"kind": "reset_colour"}}, {"text": "."}, {"control": {"kind": "choice", "choice_labels": [5500, 5501], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_goron004:Com_Talk_60": {"attributes": "", "contents": [{"text": "How does "}, {"control": {"kind": "set_colour", "colour": "red"}}, {"control": {"kind": "variable", "variable_kind": 16, "name": "Shop_SelectItemPrice"}}, {"text": " rupees"}, {"control": {"kind": "reset_colour"}}, {"text": " sound?"}, {"control": {"kind": "choice", "choice_labels": [5500, 5501], "selected_index": 0, "cancel_index": 1, "unknown": 6}}]}, "EventFlowMsg/Npc_goron004:Com_Talk_61": {"attributes": "", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Aww, that's too bad."}]}, "EventFlowMsg/Npc_goron004:near00": {"attributes": "Npc_goron004", "contents": [{"text": "Come on in."}]}, "EventFlowMsg/Npc_goron004:talk00": {"attributes": "Npc_goron004", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "You have the privilege of standing in\n"}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "Ripped and Shredded"}, {"control": {"kind": "reset_colour"}}, {"text": ", my armor shop."}, {"control": {"kind": "pause", "length": "short"}}, {"text": "\nHow can I help ya?"}, {"control": {"kind": "choice", "choice_labels": [0, 1, 2, 3], "selected_index": 0, "cancel_index": 3, "unknown": 10}}]}, "EventFlowMsg/Npc_goron004:talk01": {"attributes": "Npc_goron004", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Out late, I see."}, {"control": {"kind": "pause", "length": "short"}}, {"text": " Lucky for you,\n"}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "Ripped and Shredded "}, {"control": {"kind": "reset_colour"}}, {"text": "is open\nfor business 24 hours a day!"}, {"control": {"kind": "choice", "choice_labels": [0, 1, 2, 3], "selected_index": 0, "cancel_index": 3, "unknown": 10}}]}, "EventFlowMsg/Npc_goron004:talk02": {"attributes": "Npc_goron004", "contents": [{"control": {"kind": "sound", "unknown": [0, 1]}}, {"text": "Stop by again!"}]}, "EventFlowMsg/Npc_goron004:talk06": {"attributes": "Npc_goron004", "contents": [{"control": {"kind": "sound", "unknown": [0, 1]}}, {"text": "As you can see, this here is my\n"}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "armor shop"}, {"control": {"kind": "reset_colour"}}, {"text": "."}]}, "EventFlowMsg/Npc_goron004:talk07": {"attributes": "Npc_goron004", "contents": [{"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Do ya need anything else?"}, {"control": {"kind": "choice", "choice_labels": [0, 1, 2, 3], "selected_index": 0, "cancel_index": 3, "unknown": 10}}]}, "EventFlowMsg/Npc_goron004:talk09": {"attributes": "Npc_goron004", "contents": [{"control": {"kind": "sound", "unknown": [0, 1]}}, {"text": "Funny thing is, Gorons normally don't\nwear anything at all. We started sellin'\n"}, {"control": {"kind": "set_colour", "colour": "red"}}, {"text": "flame-resistant armor "}, {"control": {"kind": "reset_colour"}}, {"text": "for the tourists.\nI recommend the "}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "flamebreaker boots"}, {"control": {"kind": "reset_colour"}}, {"text": "\nover all else. They're made using\n"}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "gems "}, {"control": {"kind": "reset_colour"}}, {"text": "mined from this very soil.\nAs you'd expect, they have great "}, {"control": {"kind": "set_colour", "colour": "red"}}, {"text": "flame\nresistance"}, {"control": {"kind": "reset_colour"}}, {"text": ". "}, {"control": {"kind": "pause", "length": "short"}}, {"text": "Give 'em a try! They're the\nenvy of famous warriors the world over!"}]}, "EventFlowMsg/Npc_goron004:talk10": {"attributes": "Npc_goron004", "contents": [{"control": {"kind": "sound", "unknown": [1, 10]}}, {"text": "Oh... We're actually all sold out!"}, {"control": {"kind": "pause", "length": "longer"}}, {"text": "\nWe'll have to get more stock from\nold man Rohan."}]}, "EventFlowMsg/Npc_goron004:talk11": {"attributes": "Npc_goron004", "contents": [{"control": {"kind": "sound", "unknown": [7, 0]}}, {"text": "Thanks for your business, brother!"}]}, "EventFlowMsg/Npc_goron004:talk12": {"attributes": "Npc_goron004", "contents": [{"control": {"kind": "sound", "unknown": [7, 0]}}, {"text": "Thank you, brother!"}]}, "EventFlowMsg/Npc_goron004:talk13": {"attributes": "Npc_goron004", "contents": [{"control": {"kind": "sound", "unknown": [10, 22]}}, {"text": "Hey! "}, {"control": {"kind": "pause", "length": "long"}}, {"text": "You can't be so casual about stuff\nlike that!"}]}, "EventFlowMsg/Npc_goron004:talk14": {"attributes": "Npc_goron004", "contents": [{"control": {"kind": "sound", "unknown": [11, 1]}}, {"text": "With what you're wearin' right now...\nyou should really give up on goin'\nanywhere hotter than Goron City.\n"}, {"control": {"kind": "sound", "unknown": [0, 0]}}, {"text": "Places that hot are forbidden to anyone\nwho isn't a Goron anyhow."}]}, "EventFlowMsg/Npc_goron004:talk15": {"attributes": "Npc_goron004", "contents": [{"control": {"kind": "sound", "unknown": [0, 16]}}, {"text": "Just glancing at your gear, ya seem\nto be well equipped to go anywhere\u2014\neven "}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "Death Mountain"}, {"control": {"kind": "reset_colour"}}, {"text": "!\nBut even so, that's no place for a tourist."}]}, "EventFlowMsg/Npc_goron004:talk16": {"attributes": "Npc_goron004", "contents": [{"control": {"kind": "sound", "unknown": [1, 9]}}, {"text": "But ya did a great job fireproofing!\nLike that, you could even make it to\n"}, {"control": {"kind": "set_colour", "colour": "blue"}}, {"text": "Death Mountain"}, {"control": {"kind": "reset_colour"}}, {"text": ", or anywhere really."}]}, "EventFlowMsg/Npc_goron004:talk17": {"attributes": "Npc_goron004", "contents": [{"control": {"kind": "sound", "unknown": [10, 22]}}, {"text": "Hey! "}, {"control": {"kind": "pause", "length": "long"}}, {"text": "You can't go out dressed like that!\n\n\nIf ya don't hurry and change your gear,\nyou're gonna pass out from the heat!"}, {"control": {"kind": "choice", "choice_labels": [0, 1, 2, 3], "selected_index": 0, "cancel_index": 3, "unknown": 10}}]}, "EventFlowMsg/Npc_goron004:talk18": {"attributes": "Npc_goron004", "contents": [{"control": {"kind": "sound", "unknown": [10, 22]}}, {"text": "Got it. Let's make this quick!"}]}, "EventFlowMsg/Npc_goron004:talk20": {"attributes": "Npc_goron004", "contents": [{"control": {"kind": "sound", "unknown": [1, 11]}}, {"text": "Of course ya do! If ya gear up here,\nyou'll be set to stay cool at places\neven hotter than Goron City."}]}, "EventFlowMsg/Npc_goron004:talk21": {"attributes": "Npc_goron004", "contents": [{"control": {"kind": "sound", "unknown": [10, 1]}}, {"text": "Hurry up and buy some gear!"}]}, "EventFlowMsg/Npc_goron004:talk22": {"attributes": "Npc_goron004", "contents": [{"control": {"kind": "sound", "unknown": [10, 42]}}, {"text": "You can say good-bye all ya want,\nbut you'll still need to change\nyour gear! "}, {"control": {"kind": "pause", "length": "short"}}, {"text": "Go on, make it snappy!"}]}, "EventFlowMsg/Npc_goron004:talk23": {"attributes": "Npc_goron004", "contents": [{"control": {"kind": "sound", "unknown": [11, 1]}}, {"text": "This doesn't seem like a good time\nto talk..."}, {"control": {"kind": "choice", "choice_labels": [0, 1, 2, 3], "selected_index": 0, "cancel_index": 3, "unknown": 10}}]}, "EventFlowMsg/Npc_goron004:talk24": {"attributes": "Npc_goron004", "contents": [{"control": {"kind": "sound", "unknown": [4, 1]}}, {"text": "Hey, you! "}, {"control": {"kind": "pause", "length": "longer"}}, {"text": "You're gonna get burned up\nif ya go outside like that! Hurry up and\nchange your clothes!"}]}}